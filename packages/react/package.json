{
  "name": "@feedlog-ai/react",
  "version": "0.0.17",
  "description": "React bindings for Feedlog Toolkit Web Components",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
