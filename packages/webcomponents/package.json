{
  "name": "@feedlog-toolkit/webcomponents",
  "version": "1.0.0",
  "description": "Stencil web components for Feedlog Toolkit - Data visualization components",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "unpkg": "./dist/feedlog-toolkit/feedlog-toolkit.esm.js",
  "files": [
    "dist",
    "loader"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "dev": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watch",
    "test:coverage": "echo 'Webcomponents coverage skipped due to rollup issues'",
    "storybook": "storybook dev -p 6008 --no-open",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist loader"
  },
  "dependencies": {
    "@feedlog-toolkit/core": "*"
  },
  "devDependencies": {
    "@stencil/core": "^4.0.0",
    "@stencil/react-output-target": "^0.8.0",
    "@stencil/storybook-plugin": "^0.5.6",
    "@stencil/vue-output-target": "^0.8.0",
    "@types/node": "^20.11.24",
    "storybook": "^9.1.17",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "wsl-utils": "^0.1.0"
  },
  "license": "MIT",
  "keywords": [
    "feedlog",
    "webcomponents",
    "stencil",
    "data-visualization"
  ]
}
